import random
n=int(input("Enter number of teams:"))
teams=[]
for i in range(n):
   teams.append(input())
meet=int(input("Enter number of meeting between two teams:"))
matches=[]
for i in  range(n-1):
    for j in range(i+1,n):
        for k in range(meet):
            matches.append([teams[i],teams[j]])
random.shuffle(matches)
no=0
for i in matches:
    print("Matvh number:",no,":",i[0],"vs",i[1])
    no=no+1
    
